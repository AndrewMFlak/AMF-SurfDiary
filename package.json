{
  "name": "surfdiary",
  "version": "0.1.0",
  "license": "MIT",
  "main": "server.js",
  "private": true,
  "scripts": {
    "debug": "concurrently \"node --inspect-brk=43240 server.js\" \"npm run client\"",
    "start": "if-env NODE_ENV=production && npm run server || npm run start:dev",
    "server": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "seed": "node scripts/seedDb.js",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "mongoose": "^5.4.7",
    "path": "^0.12.7",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-google-maps": "^9.4.5",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.3"
  },
  "proxy": {
    "/api": {
      "target": "https://localhost:3001",
      "secure": false
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndrewMFlak/AMF-SurfDiary.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/AndrewMFlak/AMF-SurfDiary/issues"
  },
  "homepage": "https://github.com/AndrewMFlak/AMF-SurfDiary#readme"
}
